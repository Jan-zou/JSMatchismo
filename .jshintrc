{
    //settings
    "passfail": false,
    //Env
    "browser": true,
    "nonstandard": true,
    "node": true,
    "globals": {
        "ActiveXObject": true,
        "require": true,
        "define": true,
        "module":true
    },
    //Development
    "devel": false,
    "debug": false,
    //ECMAScript 5.
    "es5": true,
    "strict":false,
    "esnext":false,
    //The Good Parts.
    "eqeqeq": false,
    "eqnull": true,
    "immed": true,
    "noarg": true,
    "undef": true,
    "unused": true,
    "trailing": false,
    "boss": true,
    "evil": true,
    "shadow": true,
    "proto": true,
    "validthis": true,
    // Personal styling preferences
    "asi": false,
    "laxbreak": true,
    "laxcomma": true,
    "curly": false,
    "nonew": true,
    "sub": true,
    "loopfunc": true,
    "regexdash": true,
    "scripturl": true,
    "multistr": true
}
